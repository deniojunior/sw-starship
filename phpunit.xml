<phpunit bootstrap="./tests/bootstrap.php" colors="true">
  <filter>
    <whitelist processUncoveredFilesFromWhitelist="true">
    </whitelist>
  </filter>
  <testsuites>
    <testsuite name="sw-test-suite">
      <directory>tests</directory>
    </testsuite>
  </testsuites>
  <logging>
    <log type="coverage-html" target="html/coverage" />
  </logging>
</phpunit>
